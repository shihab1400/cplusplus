#include <vector>
#include <iostream>
#include <cmath>
using namespace std;


int main() {
    string word;
    cin >> word;
    int up = 0, low = 0;
    for(int i = 0; i<word.size(); i++) {
        if(word[i]<='Z') {
            up++;
        } else {
            low++;
        }
    }

    for(int i = 0; i<word.size(); i++) {
        if(low>=up) {
            if(word[i]<='Z') {
                word[i]+=32;
            }
        } else {
            if(word[i]>='a') {
                word[i]-=32;
            }
        }
    }
    cout << word << endl;




    return 0;
}
