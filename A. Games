#include <iostream>
#include <iomanip>
#include <cmath>
#include <vector>
#include <list>
#include <set>
#include <iterator>
#include <utility>
#include <algorithm>
using namespace std;

int main() {

    int t, c = 0;
    cin >> t;
    vector<pair<int, int>> arr;
    vector<pair<int, int>>::iterator it1;
    vector<pair<int, int>>::iterator it2;
    for(int i = 0; i<t; i++) {
        int a, b;
        cin >> a >> b;
        arr.push_back(make_pair(a, b));
    }

    for(int i = 0; i<t; i++) {
        for(int j = 0; j<t; j++) {
            if(i<=j) {
                continue;
            }
            if(arr[i].first==arr[j].second) {
                c++;
            }
            if(arr[i].second==arr[j].first) {
                c++;
            }
        }
    }
    cout << c << endl;

    return 0;
}












