#include <iostream>
#include <map>
#include <cmath>
#include <algorithm>
#include <iterator>
#include <list>
#include <set>
#include <vector>
using namespace std;

int main() {

    int t;
    cin >> t;
    while(t--) {
        int n, s1 = 0, s2;
        cin >> n;
        for(int i = 0; i<6; i++) {
            if(i==3) {
                s2 = s1;
                s1 = 0;
            }
            s1+=n%10;
            n/=10;
        }
        if(s1==s2) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }


    return 0;
}
