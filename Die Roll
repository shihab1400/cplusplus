#include <iostream>
using namespace std;
int max(int a, int b) {
    if(a>b) {
        return a;
    } else {
        return b;
    }
}
int main() {
    int x, y, a;
    cin >> x >> y;
    a = max(x,y);
    if(a==1) {
        cout << "1/1" << endl;
    } else if(a==2) {
        cout << "5/6" << endl;
    } else if(a==3) {
        cout << "2/3" << endl;
    } else if(a==4) {
        cout << "1/2" << endl;
    } else if(a==5) {
        cout << "1/3" << endl;
    } else if(a==6) {
        cout << "1/6" << endl;
    }
 
 
    return 0;
}
