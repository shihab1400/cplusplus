#include <iostream>
#include <iomanip>
#include <vector>
#include <list>
#include <iterator>
#include <cmath>
using namespace std;


int main() {
    int x, y;
    bool b = 1;
    cin >> x >> y;
    int i = x+1;
    while(1) {
        for(int j = 2; j<=(i+1)/2; j++) {
            if(i%j==0) {
                b = 0;
            }
            if(b==0) {
                break;
            }
        }
        if(b==1) {
            break;
        }
        i++;
        b = 1;
    }
    if(i==y) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }


    return 0;
}

