#include <iostream>

using namespace std;
int main() {
    int t;
    string c = "codeforces";
    cin >> t;
    while(t--) {
        char p;
        bool b = 0;
        cin >> p;
        for(int i = 0; i<c.size(); i++) {
            if(p==c[i]) {
                b = 1;
                break;
            }
        }
        if(b==0) {
            cout << "NO" << endl;
        } else {
            cout << "YES" << endl;
        }
    }
    return 0;
}
