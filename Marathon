#include <iostream>
#include <map>
#include <cmath>
#include <algorithm>
#include <iterator>
#include <list>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--) {
        int arr[4], c = 0;
        cin >> arr[0];
        for(int i = 1; i<4; i++) {
            cin >> arr[i];
            if(arr[i]>arr[0]) {
                c++;
            }
        }
        cout << c << endl;


	}



    return 0;
}
