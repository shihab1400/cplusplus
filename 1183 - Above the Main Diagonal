#include <iostream>
#include <iomanip>
#include <cmath>
#include <string>
#include <vector>
#include <ios>
#include <limits>
#include <array>
using namespace std;


int main() {
    //FROM NET SOLUTION::
    char sora;
    cin >> sora;
    float arr[12][12], sum = 0;
    int cnt = 0;
    for(int i = 0; i<12; i++) {
        for(int j = 0; j<12; j++) {
            cin >> arr[i][j];
            if(j>i) {
                sum += arr[i][j];
                cnt++;
            }
        }
    }
    if(sora == 'S') {
        cout << fixed << setprecision(1) << sum << endl; 
    } else if(sora == 'M') {
        cout << fixed << setprecision(1) << sum/cnt << endl; 
    }
    return 0;
}
