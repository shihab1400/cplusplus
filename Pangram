#include <iostream>
#include <vector>
#include <set>
#include <algorithm>
#include <utility>
#include <iterator>
using namespace std;
int main() {
    int n;
    cin >> n;
    string pan;
    cin >> pan;
    set<char> arr;
    for(int i = 0; i<n; i++) {
        if(pan[i]<=90) {
            pan[i]+=32;
        }
        arr.insert(pan[i]);
    }
    if(arr.size()==26) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }




    return 0;
}

