#include <vector>
#include <iostream>
#include <cmath>
using namespace std;


int main() {
    int numOfFrnds, heightOftheWall, minWide = 0;
    cin >> numOfFrnds >> heightOftheWall;
    int frndHeight[numOfFrnds];
    for(int i = 0; i<numOfFrnds; i++) {
        cin >> frndHeight[i];
        if(frndHeight[i]<=heightOftheWall) {
            minWide++;
        } else {
            minWide+=2;
        }
    }
    cout << minWide << endl;





    return 0;
}
