#include <iostream>
#include <iomanip>
#include <cmath>
#include <vector>
#include <list>
#include <set>
#include <iterator>
#include <utility>
#include <algorithm>
using namespace std;

int main() {

    int n;
    cin >> n;
    int arr[n];
    int ans[n];
    for(int i = 0; i<n; i++) {
        cin >> arr[i];
        ans[arr[i]-1] = i+1;
    }
    for(int i = 0; i<n; i++) {
        cout << ans[i] << " ";
    }
    cout << endl;


    return 0;
}












