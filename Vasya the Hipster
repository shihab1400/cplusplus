#include <iostream>
#include <vector>
#include <cmath>
#include <iterator>
#include <algorithm>
using namespace std;

int main() {
    int x, y;
    cin >> x >> y;
    if(x<y) {
        cout << x << " " << (y-x)/2 << endl;
    } else {
        cout << y << " " << (x-y)/2 << endl;
    }

    return 0;
}

