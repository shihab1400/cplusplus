#include <vector>
#include <iostream>
#include <cmath>
using namespace std;


int main() {
    int num, sec;
    cin >> num >> sec;
    string initial;
    cin >> initial;
    for(int i = 0; i<sec; i++) {
        for(int j = 0; j<num; j++) {
            if(initial[j] == 'B' && initial[j+1] == 'G') {
                initial[j] = 'G';
                initial[j+1] = 'B';
                j++;
            }
        }
    }
    cout << initial << endl;





    return 0;
}
