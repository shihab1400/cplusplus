#include <vector>
#include <iostream>
#include <cmath>
using namespace std;


int main() {
    int num, times;
    cin >> num >> times;
    for(int i = 0; i<times; i++) {
        int rem;
        rem = num%10;
        if(rem!=0) {
            num-=1;
        } else {
            num/=10;
        }
    }
    cout << num << endl;





    return 0;
}
